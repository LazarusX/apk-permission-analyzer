<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_marginRight="5dip" android:layout_height="fill_parent">
	<TextView android:text="TextView" android:id="@+id/permission_header_view"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
		android:textAppearance="?android:attr/textAppearanceLarge"
		android:paddingLeft="10dip" android:layout_marginTop="10dp"
		android:textSize="18sp" />

	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/linearLayout1">
		<TextView android:id="@+id/permission_descrption_view"
			android:layout_width="270dp" android:text="TextView"
			android:layout_height="wrap_content" android:paddingLeft="10dip"
			android:layout_marginBottom="6dp" android:ellipsize="marquee"
			android:singleLine="true">
		</TextView>
		<TextView android:id="@+id/permission_percent_view"
			android:paddingLeft="1dip" android:layout_width="wrap_content"
			android:text="90%" android:paddingRight="6dip" android:layout_height="wrap_content"
			android:layout_marginBottom="6dp" android:gravity="right"
			android:background="#006400" android:textColor="#000000">
		</TextView>
	</LinearLayout>
</LinearLayout>
